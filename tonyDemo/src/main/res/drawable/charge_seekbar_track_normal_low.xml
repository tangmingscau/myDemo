<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background"
          android:drawable="@drawable/charge_track_nocharge_normal_bg">

    </item>
    <item android:id="@android:id/progress"
    >
        <clip>
            <shape android:shape="rectangle">
                <!-- 显示一条虚线，破折线的宽度为dashWith，破折线之间的空隙的宽度为dashGap，当dashGap=0dp时，为实线 -->
                <stroke android:width="5dp" android:color="#8CFC40"
                        android:dashWidth="15dp" android:dashGap="5dp" />
                <corners android:radius="8dp"></corners>
                <gradient
                    android:centerColor="#9FBEA8"
                    android:endColor="#9FBEA8"
                    android:startColor="#9FBEA8"
                    android:type="linear"
                ></gradient>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress"
    >
        <clip android:drawable="@drawable/charge_track_nocharge_low_secondprogress"></clip>
    </item>
</layer-list>